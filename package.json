  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "reset": "react-native start --reset-cache",
    "prettier": "prettier --write \"./**/*.{js,ts,tsx,scss}\"",
    "app-build": "cd ./android && rm -rf app/src/main/res/drawable* && ./gradlew app:assembleRelease",
    "S": "react-native start",
    "clean": "cd android && ./gradlew clean && cd ..",
    "xcode": "cd ios && open zoul.xcworkspace",
    "pods": "cd ios && pod install && cd ..",
    "androidBundle": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "apkD": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/ && cd android && ./gradlew assembleDebug",
    "iosBundle": "npx react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'",
    "aab": "cd android && ./gradlew bundleRelease && cd ..",
    "apk": "cd android && ./gradlew assembleRelease"
  },
